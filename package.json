{
  "name": "pzl-stream-lib",
  "version": "1.4.12",
  "description": "Internal Streaming library for Puzzel",
  "license": "UNLICENSED",
  "main": "dist/pzl-stream-lib.js",
  "module": "src/PzlStream.js",
  "engines": {
    "node": ">=0.10.24"
  },
  "scripts": {
    "prebuild": "rimraf -g dist/*",
