    "babel-loader": "^9.1.2",
    "cross-env": "^7.0.3",
    "onchange": "^7.1.0",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
