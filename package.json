{
  "name": "pzl-stream-lib",
  "version": "1.7.1",
  "description": "Internal Streaming library for Puzzel",
  "license": "UNLICENSED",
  "main": "dist/pzl-stream-lib.min.js",
  "module": "src/PzlStream.js",
  "types": "./types.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "browser": "./dist/pzl-stream-lib.min.js",
      "import": "./dist/pzl-stream-lib.min.js",
      "default": "./dist/pzl-stream-lib.min.js"
    },
    "./debug": {
      "browser": "./dist/pzl-stream-lib.js",
      "import": "./dist/pzl-stream-lib.js",
      "default": "./dist/pzl-stream-lib.js"
    },
    "./node": "./dist/pzl-stream-lib.node.min.cjs",
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist/**",
    "types.d.ts",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "npm run build:prod && npm run build:dev",
    "build:dev": "webpack -c webpack.dev.config.js",
    "build:prod": "webpack -c webpack.prod.config.js",
    "build:prod:browser": "webpack -c webpack.prod.browser.config.js",
    "build:prod:node": "webpack -c webpack.prod.node.config.js",
    "watch": "onchange \"src/**/*.js\" -- npm run build:dev",
    "prebuild": "rimraf -g dist/*",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "stately.js": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/plugin-transform-optional-chaining": "^7.28.6",
    "@babel/plugin-transform-runtime": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/runtime": "^7.28.6",
    "@microsoft/signalr": "^6.0.0",
    "babel-cli": "^7.0.0-beta.3",
    "babel-jest": "^30.2.0",
    "babel-loader": "^10.0.0",
    "core-js": "^3.48.0",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "onchange": "^7.1.0",
    "rimraf": "^6.1.3",
    "terser-webpack-plugin": "^5.3.16",
    "webpack": "5.105.2",
    "webpack-cli": "^6.0.1"
  },
  "peerDependencies": {
    "@microsoft/signalr": ">=6.0.0"
  }
}
