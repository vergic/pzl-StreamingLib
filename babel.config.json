{
  "presets": [
    ["@babel/env", {
      "modules": false,
      "targets": { "edge": "17", "firefox": "60", "chrome": "67", "safari": "11.1", "node": "current" },
      "useBuiltIns": "usage",
      "corejs": 3
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-optional-chaining",
    ["@babel/plugin-transform-runtime", { "useESModules": true }]
  ],
  "env": {
    "test": {
      "presets": [
        ["@babel/env", {
          "modules": "commonjs",
          "targets": { "node": "current" },
          "useBuiltIns": "usage",
          "corejs": 3
        }]
      ],
      "plugins": [
        "@babel/plugin-transform-optional-chaining",
        ["@babel/plugin-transform-runtime", { "useESModules": false }]
      ]
    }
  }
}
